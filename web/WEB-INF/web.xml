<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <servlet>
        <servlet-name>IndexInit</servlet-name>
        <servlet-class>servlet.IndexInit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexInit</servlet-name>
        <url-pattern>/IndexInit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FilmInit</servlet-name>
        <servlet-class>servlet.FilmInit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FilmInit</servlet-name>
        <url-pattern>/FilmInit</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
